#!nsh
#
# @name BoxWing
#
# @type Tailsitter
#
# @maintainer Roman Bapst <roman@px4.io>
#

sh /etc/init.d/rc.vtol_defaults

set MIXER boxwing
set MIXER_AUX boxwing_yaw
set PWM_OUT 1234
set PWM_RATE 400
set PWM_AUX_DISARMED 1000

set MAV_TYPE 20

param set VT_MOT_COUNT 4
param set VT_IDLE_PWM_MC 1080
param set VT_TYPE 0